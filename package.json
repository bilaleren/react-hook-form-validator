{
  "name": "react-hook-form-validator",
  "version": "1.0.0",
  "description": "React Hook Form field validator.",
  "main": "./dist/index.cjs.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bilaleren/react-hook-form-validator"
  },
  "bugs": {
    "url": "https://github.com/bilaleren/react-hook-form-validator/issues",
    "email": "bilal@webnoi.com"
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "test:watch": "yarn run test --watchAll",
